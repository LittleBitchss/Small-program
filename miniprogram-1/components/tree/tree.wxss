.tree_left{
  position: relative;
  float: left;
}

.icon-down{
  border: 8px solid;
  border-color: transparent transparent #B8B8B8;
  transform: rotate(180deg);
  margin-top: 7px;
  margin-left: 5px;
  margin-right: 10px;
}

.icon-right{
  border: 8px solid;
  border-color: transparent transparent #B8B8B8;
  transform: rotate(90deg);
  margin-left: 5px;
}

.selected{
  height: 20px;
  width: 20px;
  border-radius: 50%;
  border: 1px solid #D6D6D6;
  float: right;
  margin-left: 5px;
  margin-top: 2px;
}
.chef{
  width: 198rpx;
  height: 140rpx;
  border-radius: 20rpx;
  background-color: #fff;
  box-shadow: 0rpx 0rpx 8rpx -3rpx #000;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-family: 'Normal';
  font-size: 24rpx;
}
.img{
  width: 98rpx;
  height: 98rpx;
  border-radius: 50%;
}