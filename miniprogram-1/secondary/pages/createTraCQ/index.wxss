/* secondary/pages/createTraCQ/index.wxss */
.box{
  height: 100vh;
  display: flex;
  flex-direction: column;
}
.createTraCQ{
  width: 100%;
  flex: 1;
  position: relative;
}
.createTraCQ .title{
  font-size: 45rpx;
  font-family: 'Medium';
  padding: 66rpx 38rpx 0;
}
.createTraCQ .photo{
  height: 168rpx;
  margin: 0 24rpx;
  padding: 0 36rpx;
  border-bottom: 2rpx solid #cfcfcf;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 25rpx;
  font-family: 'Regular';
}
.createTraCQ .photo .photo_img{
  width: 114rpx;
  height: 114rpx;
  background-color: #fff;
  border-radius: 50%;
  display: block;
  box-shadow: #0186fe 0rpx 0rpx 20rpx -8rpx;
  margin: 0 0 14rpx;
  overflow: hidden;
  background-image: url(https://qunyan.canancn.com/assets/applet/img/jobRecruitment/photo.png);
  background-size: 100% 100%;
}
.createTraCQ .photo .photo_img .photo_img_s{
  height: 100%;
  width: 100%;
}
.createTraCQ .item{
  height: 168rpx;
  margin: 0 24rpx;
  padding: 0 36rpx;
  border-bottom: 2rpx solid #cfcfcf;
  display: flex;
  justify-content: center;
  font-size: 25rpx;
  font-family: 'Regular';
  flex-direction: column;
}
.createTraCQ .item .item_title{
  width: 100%;
  font-size: 25rpx;
  font-family: 'Regular';
  margin-bottom: 24rpx;
}
.createTraCQ .item .item_conts{
  width: 100%;
  font-size: 30rpx;
  display: flex;
  color: #b6b6b6;
  justify-content: space-between;
  align-items: center;
}

.createTraCQ .item .item_conts .item_conts_inp{
  width: 100%;
  color: #000;
}
.createTraCQ .item .item_conts .placeholder{
  color: #b6b6b6;
}
.createTraCQ .item .item_conts .item_conts_img{
  width: 14rpx;
  height: 30rpx;
}
.createTraCQ .item .item_conts .active{
  color: #000;
}

.createTraCQ .bottom{
  width: 100%;
  height: 100rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-family: 'Medium';
}
.createTraCQ .bottom .bottom_btn{
  height: 60rpx;
  width: 702rpx;
  background-color: #a3b3f9;
  color: #fff;
  font-size: 28rpx;
}
.bottom_click{
  font-family: 'Medium';
  font-size: 20rpx;
  padding:  0 0 20rpx;
  display: flex;
  justify-content: center;
}
.createTraCQ .bottom .actives{
  background-color: #4781d8;
}
