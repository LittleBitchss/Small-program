/* secondary/pages/publishPageTwe/index.wxss */
.publishPageTwe{
  height: 100vh;
}
.publishPageTwe .title{
  height: 110rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-family: 'Medium';
  font-size: 45rpx;
  margin: 0 30rpx;
}
.publishPageTwe .course{
  position: relative;
  height: 86rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-family: 'Light';
  font-size: 20rpx;
  line-height: 1;
  margin: 0 30rpx;
}
.publishPageTwe .course .course_item{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  height: 70rpx;
}
.publishPageTwe .course .course_midden{
  z-index: 0;
  width: 582rpx;
  height: 2rpx;
  background-color: #f14d45;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translatex(-50%);
  margin-top: 22rpx;
}
.publishPageTwe .course .course_item .outerRace{
  z-index: 1;
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  background-color: #f14d45;
  display: flex;
  justify-content: center;
  align-items: center;
}
.publishPageTwe .course .course_item .insideTracks{
  z-index: 0;
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background-color: #f14d45;
  margin-top: 9rpx;
}
.publishPageTwe .course .course_item .outerRace .insideTrack{
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background-color: #fff;
}
.publishPageTwe .item{
  height: 164rpx;
  margin: 0 22rpx;
  padding: 0 38rpx;
  border-bottom: 2rpx solid #cfcfcf;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.publishPageTwe .item .item_title{
  font-family: 'Regular';
  font-size: 25rpx;
  margin-bottom: 22rpx;
}
.publishPageTwe .item .item_cont{
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #5f5f5f;
  font-family: 'Regular';
  font-size: 30rpx;
}
.publishPageTwe .item .item_cont .active{
  color: #000;
  font-family: 'Medium';
}
.publishPageTwe .item .item_cont .item_cont_txt{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-right: 20rpx;
}
.publishPageTwe .item .item_cont .item_cont_img{
  width: 16rpx;
  height: 30rpx;
}




.publishPageTwe .bottom{
  height: 128rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  left: 0;
  bottom: 0;
}

.publishPageTwe .bottom .btn{
  height: 60rpx;
  width: 702rpx;
  background-color: #a3b3f9;
  color: #fff;
  font-family: 'Medium';
  font-size: 28rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.publishPageTwe .bottom .actives{
  background-color: #4768f3;
}

.page-container .keyWord{
  height: 100%;
  overflow: auto;
  padding-bottom: 138rpx;
}
.page-container .keyWord .title{
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  font-family: 'Medium';
  font-size: 45rpx;
  margin: 0 30rpx;
}
.page-container .keyWord .note{
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  font-family: 'Medium';
  font-size: 20rpx;
  margin: 0 30rpx;
  color: #5f5f5f;
  margin-bottom: 46rpx;
}
.page-container .keyWord .item{
  margin: 0 45rpx;
}
.page-container .keyWord .item .item_title{
  font-family: 'Medium';
  font-size: 30rpx;
}
.page-container .keyWord .item .item_cont{
  padding: 0 0 34rpx 0;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.page-container .keyWord .item .item_cont .item_cont_item{
  width: 200rpx;
  height: 50rpx;
  border-radius: 5rpx;
  background-color: #f0f0f0;
  display: flex;
  justify-content: center;
  align-items: center;
  font-family: 'Normal';
  font-size: 25rpx;
  margin: 35rpx 30rpx 0 0;
  position: relative;
}
.page-container .keyWord .item .item_cont .item_cont_item .item_cont_item_img{
  overflow: hidden;
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  border-radius: 50%;
  background-color: #999;
  top: -6rpx;
  right: -6rpx;
}
.page-container .keyWord .item .item_cont .active{
  background-color: #fff;
  border: 2rpx solid #4768f3;
}

.page-container .keyWord .item .item_cont .add{
  color: #4768f3;
}
.page-container .keyWord .item .item_cont .item_cont_item:nth-child(3n){
  margin-right: 0;
}


.page-container .keyWord .bottom{
  width: 100%;
  height: 138rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 35rpx;
  font-family: 'Regular';
  background-color: #fff;
  border-top: 2rpx solid #eee;
}
.page-container .keyWord .bottom .empty{
  width: 204rpx;
  height: 70rpx;
  border-radius: 5rpx;
  background-color: #f0f0f0;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 28rpx;
}
.page-container .keyWord .bottom .confirm{
  width: 478rpx;
  height: 70rpx;
  border-radius: 5rpx;
  background-color: #4768f3;
  display: flex;
  color: #fff;
  justify-content: center;
  align-items: center;
}
.page-container .WorkAddress{
  height: 100%;
  position: relative;
}
.page-container .WorkAddress .map{
  width: 100%;
  height: calc(100% - 300rpx);
  overflow: hidden;
}
.page-container .WorkAddress .maps{
  width: 100%;
  height: 120%;
  margin-top: -10%;
}
.page-container .WorkAddress .maps .cover-image {
  height: 50rpx;
  width: 36rpx;
  position: absolute;
  z-index: 200;
  top: 50%;
  left: 50%;
  margin-top: -50rpx;
  margin-left: -18rpx;
}
@keyframes show_ani{
  0%{
    transform: scale(1);
  }
  33%{
    transform: scale(1.2);
  }
  66%{
    transform: scale(0.5);
  }
  100%{
    transform: scale(1);
  }
}
.page-container .WorkAddress .maps .show_ani{
  animation: show_ani 1s; 
}
.page-container .WorkAddress .search{
  width: 80%;
  height: 60rpx;
  border-radius: 100rpx;
  background-color: #fff;
  position: absolute;
  top: 30rpx;
  left: 50%;
  transform: translateX(-50%);
  box-shadow: 0 1rpx 8rpx -3rpx #305181;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
}
.page-container .WorkAddress .search .search_img{
  width: 38rpx;
  height: 40rpx;
}
.page-container .WorkAddress .search .search_inp{
  flex: 1;
  padding: 0 20rpx;
  font-size: 25rpx;
}
.page-container .WorkAddress .search .placeholder{
  color: #305181;
  font-family: Regular;
  font-size: 25rpx;
}
.page-container .WorkAddress .bottoms{
  height: 300rpx;
  font-family: 'Regular';
  font-size: 30rpx;
}
.page-container .WorkAddress .bottoms .workAddress{
  height: 100rpx;
  border-bottom: 2rpx solid #eee;
  display: flex;
  align-items: center;
}
.page-container .WorkAddress .bottoms .workAddress .workAddress_left{
  width: 200rpx;
  display: flex;
  justify-content: flex-end;
  color: #666;
}
.page-container .WorkAddress .bottoms .workAddress .workAddress_right{
  flex: 1;
  margin: 0 20rpx; 
}
.page-container .WorkAddress .bottoms .fullAddress{
  height: 100rpx;
  display: flex;
  align-items: center;
  padding-left: 200rpx;
  border-bottom: 2rpx solid #eee;
}
.page-container .WorkAddress .bottoms .fullAddress .fullAddress_inp{
  flex: 1;
  margin: 0 20rpx;
}
.page-container .WorkAddress .bottoms .bottoms_bottom{
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.page-container .WorkAddress .bottoms .bottoms_bottom .btn{
  height: 60rpx;
  width: 702rpx;
  background-color: #a3b3f9;
  color: #fff;
  font-family: 'Medium';
  font-size: 28rpx;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.page-container .WorkAddress .bottoms .bottoms_bottom .active{
  background-color: #4768f3;
}
.page-container .WorkAddress .showview{
  z-index: 100;
  width: 100%;
  height: 60%;
  background-color: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  overflow: auto;
}
.page-container .WorkAddress .showview .showview_item{
  height: 160rpx;
  border-bottom: 2rpx solid #eee;
  margin: 0 30rpx;
  padding: 20rpx 50rpx;
  font-family: 'Regular';
  font-size: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.page-container .WorkAddress .showview .showview_item .showview_item_top{
  display: flex;
  align-items: center;
}
.page-container .WorkAddress .showview .showview_item .showview_item_bom{
  display: flex;
  align-items: center;
  font-size: 25rpx;
  color: #555;
}
